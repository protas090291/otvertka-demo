# Supabase Configuration (бэкенд / офис-агент)
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
SUPABASE_ANON_KEY=your_anon_key_here

# Фронт (Vite): для сборки и деплоя — копируйте в .env.production
# При сборке: npm run build подхватит эти переменные (только с префиксом VITE_)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_anon_key_here
VITE_SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Backend Configuration
BACKEND_URL=http://localhost:8000

# Office Agent Configuration
PRINTER_NAME=your_printer_name_here

# Mobile App Configuration (для React Native)
API_BASE_URL=http://localhost:8000

# Optional: Voice Recognition
VOICE_RECOGNITION_API_KEY=your_voice_api_key_here

# AI Assistant (OpenRouter) — без ключа нейросеть в чате не будет отвечать
# Получить ключ: https://openrouter.ai/keys
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here


